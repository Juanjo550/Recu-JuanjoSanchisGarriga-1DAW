<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.recuproyecto.Controladores.NuevoContacto"
            prefHeight="600.0" prefWidth="900.0" >


    <children>
        <VBox alignment="CENTER" prefHeight="600.0" prefWidth="900.0">
            <children>
                <Label prefHeight="33.0" prefWidth="52.0" text="Nombre">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="tfNombre" onAction="#anadirNombre" prefHeight="42.0" prefWidth="627.0" />
