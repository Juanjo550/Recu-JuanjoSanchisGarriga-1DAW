                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="tfApellido1" onAction="#anadirApellido1" prefHeight="42.0" prefWidth="627.0" />
                <Label prefHeight="34.0" prefWidth="70.0" text="Apellido 2">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="tfApellido2" onAction="#anadirApellido2" prefHeight="42.0" prefWidth="627.0" />
                <Label prefHeight="34.0" prefWidth="62.0" text="Telefono">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="tfTelefono" onAction="#anadirTelefono" prefHeight="42.0" prefWidth="627.0" />
                <Label prefHeight="33.0" prefWidth="52.0" text="Email">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="tfEmail" onAction="#anadirEmail" prefHeight="42.0" prefWidth="627.0" />
                <Button fx:id="btAnadirContacto" mnemonicParsing="false" onAction="#anadirContacto" prefHeight="34.0" prefWidth="302.0" text="AÃ±adir contacto">
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="btVolver" mnemonicParsing="false" onAction="#Volver" text="Volver">
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Button>
            </children>
            <padding>
                <Insets left="150.0" right="150.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
